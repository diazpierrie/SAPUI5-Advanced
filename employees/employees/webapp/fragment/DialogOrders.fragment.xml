<core:FragmentDefinition xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Dialog id="_IDGenDialog1-Dialog" title="{i18n>orders}">
        <Table id="_IDGenTable1-Dialog" items="{odataNorthwind>Orders}" width="700px">
            <columns>
                <Column id="_IDGenColumn1-Dialog">
                    <header>
                        <Label id="_IDGenLabel1-Dialog" text="{i18n>orderID}" />
                    </header>
                </Column>
                <Column id="_IDGenColumn2-Dialog">
                    <header>
                        <Label id="_IDGenLabel2-Dialog" text="{i18n>freight}" />
                    </header>
                </Column>
                <Column id="_IDGenColumn3-Dialog">
                    <header>
                        <Label id="_IDGenLabel3-Dialog" text="{i18n>shipAddress}" />
                    </header>
                </Column>
            </columns>
            <items>
                <ColumnListItem id="_IDGenColumnListItem1-Dialog">
                    <cells>
                        <Label id="_IDGenLabel4-Dialog" text="{odataNorthwind>OrderID}"></Label>
                        <Label id="_IDGenLabel5-Dialog" text="{odataNorthwind>Freight}"></Label>
                        <Label id="_IDGenLabel6-Dialog" text="{odataNorthwind>ShipAddress}"></Label>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <endButton>
            <Button id="_IDGenButton1-Dialog" text="{i18n>close}" press="onCloseOrders"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>